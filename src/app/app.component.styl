*
	margin 0
	padding 0

body
	background-color #2b2b2b
	color #a5c261

body .wrap
	width 100%
	max-width 960px
	min-height 500px
	margin 0 auto
	padding 40px 20px
	font 16px Arial, sans-serif

body .wrap h2
	color #0060ff
	font 30px Arial, sans-serif
	margin auto auto 20px
	text-align center

.clear
	clear both
